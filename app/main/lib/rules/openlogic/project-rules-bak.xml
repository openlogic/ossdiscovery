<project-rules universal-version="20080515-165933">
  <project-rule name="fast-file-name-matcher" from="wild" os="all">
    <description>Looks for file name matches as defined in projects.txt</description>
    <eval expression="all" speed="1"/>
    <match-rules name="all">
      <result expression="ternarytree"/>
      <match-rule name="ternarytree" type="filenameList" filename=".*" projectfile="projects.txt"/>
    </match-rules>
  </project-rule>
</project-rules>